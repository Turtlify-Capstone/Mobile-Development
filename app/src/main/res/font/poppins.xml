        android:fontStyle="italic"
        android:fontWeight="500"
        android:font="@font/poppinsmediumitalic" />
    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/poppinsregular" />
    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/poppinsitalic" />
    <font
        android:fontStyle="italic"
        android:fontWeight="300"
        android:font="@font/poppinslight" />
    <font
        android:fontStyle="italic"
        android:fontWeight="300"
        android:font="@font/poppinslightitalic" />
    <font
        android:fontStyle="normal"
        android:fontWeight="200"
        android:font="@font/poppinsextralight" />
    <font
        android:fontStyle="italic"
        android:fontWeight="200"
        android:font="@font/poppinsextralightitalic" />
    <font
        android:fontStyle="italic"
        android:fontWeight="100"
        android:font="@font/poppinsthin" />
    <font
        android:fontStyle="normal"
        android:fontWeight="100"
        android:font="@font/poppinsthinitalic" />
</font-family>

